{
  "name": "@privy-io/crypto",
  "version": "0.0.1",
  "description": "Privy encryption and decryption library",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "version": "git add package.json package-lock.json && git commit -m \"@privy-io/crypto@${npm_package_version}\""
  },
  "keywords": [
    "privy",
    "encryption",
    "decryption",
    "crypto",
    "cryptography",
    "e2ee",
    "privacy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "events": "3.3.0",
    "webcrypto": "0.1.1"
  }
}
