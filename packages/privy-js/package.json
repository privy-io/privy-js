{
  "name": "@privy-io/privy-js",
  "version": "0.0.1",
  "description": "Browser client for the Privy API",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "privy",
    "privacy",
    "storage",
    "user data",
    "encryption",
    "decryption",
    "cryptography",
    "web3"
  ],
  "license": "Apache-2.0",
  "homepage": "https://privy.io",
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest --testMatch \"**/test/unit/**/*.test.ts\"",
    "test-integration": "jest --testMatch \"**/test/integration/**/*.test.ts\""
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/md5": "^2.3.2",
    "jest": "^27.5.1",
    "jsonwebtoken": "^8.5.1",
    "prettier": "^2.6.1",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@ethersproject/address": "^5.6.0",
    "@privy-io/crypto": "0.0.3",
    "axios": "^0.26.1",
    "base64-js": "^1.5.1",
    "dotenv": "^16.0.0",
    "jwt-decode": "^3.1.2",
    "md5": "^2.3.0"
  }
}
