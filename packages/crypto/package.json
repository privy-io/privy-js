{
  "name": "@privy-io/crypto",
  "version": "0.0.2",
  "description": "Privy encryption and decryption library",
  "main": "dist/node.js",
  "browser": "dist/browser.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run clean && npm run build",
    "prettier": "prettier --check \"src/**/*.ts\"",
    "test": "jest --testMatch \"**/test/*.test.ts\"",
    "test-webcrypto": "jest --testMatch \"**/test/webcrypto/*.test.ts\""
  },
  "keywords": [
    "privy",
    "encryption",
    "decryption",
    "crypto",
    "cryptography",
    "e2ee",
    "privacy"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
